//! A node that sets an element at a specific index.

/// Set an element at a specific index.
fn @[pulsar_node_fn_id]@() -> Vec<@[in_item_type]@> {
    let mut array = @[in_array_vector]@;
    let index = @[in_index_number]@ as usize;
    if index < array.len() {
        array[index] = @[in_value_any]@;
    }
    array
}