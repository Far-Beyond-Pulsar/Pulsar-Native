//! A logging node that outputs an info message.

/// Log an info message with timestamp.
fn @[pulsar_node_fn_id]@() {
    let now = std::time::SystemTime::now()
        .duration_since(std::time::UNIX_EPOCH)
        .unwrap()
        .as_secs();
    println!("[INFO {}] {}", now, @[in_message_string]@);
}