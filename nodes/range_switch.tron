//! A node that executes branches based on numeric ranges.

/// Execute different branches based on numeric ranges.
fn @[pulsar_node_fn_id]@() {
    let value = @[in_value_number]@;
    if value < 0.0 {
        @[pulsar_exec_negative]@
    } else if value < 10.0 {
        @[pulsar_exec_low]@
    } else if value < 50.0 {
        @[pulsar_exec_medium]@
    } else if value < 100.0 {
        @[pulsar_exec_high]@
    } else {
        @[pulsar_exec_extreme]@
    }
}