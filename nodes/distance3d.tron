//! A node that calculates the distance between two 3D points.

/// Calculate the distance between two 3D points.
fn @[pulsar_node_fn_id]@() -> f32 {
    let (x1, y1, z1) = @[in_a_vector3]@;
    let (x2, y2, z2) = @[in_b_vector3]@;
    let dx = x2 - x1;
    let dy = y2 - y1;
    let dz = z2 - z1;
    (dx * dx + dy * dy + dz * dz).sqrt()
}