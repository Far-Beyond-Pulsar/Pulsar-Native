//! A pure function that checks if a value is within a specified range.

/// Check if a value is within a specified range (inclusive).
fn @[pulsar_node_fn_id]@() -> bool {
    let value = @[in_value_number]@;
    let min = @[in_min_number]@;
    let max = @[in_max_number]@;
    value >= min && value <= max
}